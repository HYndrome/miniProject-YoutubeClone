mixin video(video)
  div
    h4
      //- a(href="/video/" + video.id)=video.title
      a(href=`/video/${video.id}`)=video.title
    div.video-mixin__thumb(style=`background-image:url(${video.thumbUrl}); width: 200px; height: 200px`)
    p=video.description
    ul 
      each hashtag in video.hashtags
        li=hashtag
    small=video.createdAt
    hr